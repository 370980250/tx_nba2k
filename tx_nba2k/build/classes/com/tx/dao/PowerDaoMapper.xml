<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tx.dao.PowerDao">
	<select id="queryPower" parameterType="int" resultType="power">
		select * from tx_power where id=#{pid}
	</select>
	
	<select id="queryPowerList" resultType="power">
		select * from tx_power order by id asc
	</select>
</mapper>